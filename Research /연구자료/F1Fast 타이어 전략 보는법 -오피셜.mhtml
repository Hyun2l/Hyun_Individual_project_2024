<span class=3D"n">session</span><span class=3D"o">.</span><span class=3D"n"=
>load</span><span class=3D"p">()</span>
<span class=3D"n">laps</span> <span class=3D"o">=3D</span> <span class=3D"n=
">session</span><span class=3D"o">.</span><span class=3D"n">laps</span>
